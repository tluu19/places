<h1>{{ title }}</h1>
<div class='card-container'>
    {{#each places}}
    <div class= 'cards card-{{@index}}'>
        <div class= 'card-text'>
          <div class='card-name'>{{ name }}</br> </div>
          <div class='card-location'> in {{ location }}  </div>
        </div>
    </div>
    {{/each}}
</div>
